    tools:context="com.av.lenovo.sa3edny.fragments.ContactUsFragment">
    <!-- TODO: Update blank fragment layout -->

    <include layout="@layout/path_layout" />
<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

